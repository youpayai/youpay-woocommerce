{
    "name": "mywork/woo-youpay",
    "description": "YouPay",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "Clara Leigh",
            "email": "clara@mywork.com.au"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:MyWorkAus/youpay-php-sdk.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "youpayai/youpay-php-sdk": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^v3.4",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "classmap": [
          "controllers",
            "bootstrap"
        ],
        "psr-4": {
            "WooYouPay\\": "./"
        }
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    }
}
